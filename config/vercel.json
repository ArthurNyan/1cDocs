{
  "version": 2,
  "name": "1c-docs-project",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "outputDirectory": "dist",
  "buildCommand": "cd scripts && bash generate_web_simple.sh",
  "devCommand": "python3 -m http.server 8000 --directory dist",
  "installCommand": "echo 'No dependencies to install'",
  "cleanUrls": true,
  "trailingSlash": false
}
